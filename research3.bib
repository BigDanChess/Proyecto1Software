@article{moralesvalenciaInvestigacionMercadosPara2022,
  title = {Investigación de Mercados Para Conocer La Calidad Del Servicio al Cliente de La {{Peluquería Sofy}}’s En El Municipio de {{Floridablanca}}, Año 2021},
  author = {Morales Valencia, María Camila and Ojeda Nino, Angie Natalia and Valencia Centeno, María Camila},
  date = {2022-01-14T13:31:08Z},
  url = {http://repositorio.uts.edu.co:8080/xmlui/handle/123456789/8430},
  urldate = {2022-04-23},
  abstract = {El presente proyecto de investigación pretende conocer la calidad del servicio al cliente de la Peluquería Sofy’s en el municipio de Floridablanca, en el año 2021, con la finalidad de conocer todo lo relacionado con el servicio al cliente tales como quejas, reclamos, inquietudes, inconvenientes encontrados por los clientes, aspectos que se deben mejorar, calidad de la asesoría y tiempo de resolución de solicitudes, entre otros factores. La metodología utilizada fue una revisión de literatura científica y la aplicación de un instrumento de tipo investigación descriptiva a través de la realización de una encuesta (n=421), que tuvo como objetivo describir sistemáticamente las apreciaciones de los clientes en relación con la calidad de servicio de la Peluquería Sofy’s en el municipio de Floridablanca. La técnica estadística que se uso fue la aplicación de las funciones asimetría y curtosis para 2 de las 12 preguntas de la encuesta. Los resultados obtenidos permitieron evidenciar las problemáticas encontradas a nivel del servicio al cliente de la Peluquería Sofy’s en el municipio de Floridablanca. Las conclusiones permitieron obtener informaciones que podrán ayudar esta microempresa en su toma de decisión con recomendaciones para mejorar la calidad del servicio al cliente de esta misma.},
  langid = {english},
  annotation = {Accepted: 2022-01-14T13:31:08Z}
}

