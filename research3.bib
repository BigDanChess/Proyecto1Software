@video{aprendemosjuntosComoSuperarMiedo2022,
  title = {Cómo Superar El Miedo Escénico. {{Victoria Siedlecki}}, Actriz y Cuentacuentos},
  editor = {{AprendemosJuntos}},
  date = {2022-02-28},
  series = {Aprendamos {{Juntos}}},
  url = {https://www.youtube.com/watch?v=WQIeiRjkYb8},
  urldate = {2022-04-24},
  abstract = {Entra en nuestra web: https://www.bbvaaprendemosjuntos.com/es Suscríbete a nuestro canal de youtube: https://www.youtube.com/channel/UCI6Q... Síguenos en Facebook: http://fb.com/aprendemosjuntosBBVA Síguenos en Twitter: http://twitter.com/aprenderjuntos\_ Escucha nuestros podcasts en Spotify: https://open.spotify.com/show/07rXxtO... ¿Qué provoca el miedo escénico? ¿Es posible superarlo? La actriz y narradora oral Victoria Siedlecki comparte algunos trucos para dominar estas situaciones de incertidumbre. "Hay que poner el foco en la historia que vamos a contar", afirma Siedlecki. Esta intérprete, enamorada de las palabras, recalca así la importancia de las historias: "Es importante la historia, porque es el lugar en el que el público y quien cuenta se van a encontrar. Es un lugar sagrado y hermoso".  En este vídeo, la cuentacuentos comparte, además, algunas herramientas adquiridas sobre las tablas: desdoblarse, disfrutar o pensar al público como individuos muy parecidos a uno mismo, son ideas útiles para enfrentar el miedo. Por último, recuerda la actriz, hay dos situaciones que provocan miedo escénico: quedarse en blanco y perder el hilo. Pero, asegura Siedlecki, es importante confiar: "Estamos llenos de palabras que vendrán a rescatarnos".  Victoria Siedlecki es actriz y cuentacuentos. Formada en danza, interpretación y canto en la Escuela de Teatro Musical de Julio Bocca, en Argentina, se trasladó a España hace más de dos décadas. Su espectáculo ‘Relatos eróticos’ -un monólogo en el que interpretaba textos de Cortázar, Benedetti o pasajes desconocidos de ‘Las mil y una noches’- consiguió permanecer ocho temporadas en la cartelera teatral española y acercó la narración oral a más de 50.000 personas. Siedlecki colabora con diversos medios de comunicación, imparte talleres de storytelling y cuentacuentos y continúa con representaciones orales en España y América Latina. Esta experta en comunicación no verbal, dirige ‘La Escuela de Cuentacuentos’ en Madrid, donde enseña a contar cuentos y forma a narradores orales, y es cofundadora de ‘Zelwa Storytelling’, una consultora que traslada su experiencia en los escenarios a la comunicación de organizaciones y equipos.},
  editortype = {director}
}

@video{aprendemosjuntosCompletaQueOcurre2019,
  title = {V.{{Completa}}: ¿{{Qué}} Ocurre En Nuestro Cerebro Cuando Nos Enamoramos? {{Helen Fisher}}, Neurobióloga},
  shorttitle = {V.{{Completa}}},
  editor = {{AprendemosJuntos}},
  date = {2019-11-04},
  url = {https://www.youtube.com/watch?v=THyb-x0C350},
  urldate = {2022-04-24},
  abstract = {Entra en nuestra web: http://aprendemosjuntos.elpais.com Suscríbete a nuestro canal de youtube: https://www.youtube.com/channel/UCI6Q... Síguenos en Facebook: http://fb.com/aprendemosjuntosBBVA Síguenos en Twitter: http://twitter.com/aprenderjuntos\_ ¿Por qué nos enamoramos? ¿Y por qué sentimos atracción por unas personas y no por otras? Poner el amor bajo el microscopio de la ciencia, ese ha sido el objetivo de la Neurobióloga y Antropóloga Helen Fisher durante casi 30 años. “A lo largo de la historia, los seres humanos han sido capaces de componer canciones por amor, cantar por amor, bailar por amor. De vivir, morir y matar por amor”, explica. ¿Qué es lo que pasa entonces en el cerebro humano? Ella es la persona del mundo que más tiempo ha dedicado a estudiarlo desde la perspectiva de la neurobiología.  Nacida en Nueva York, Helen Fisher es profesora de investigación de la Rutgers University de New Jersey. Es la mayor experta del mundo en matrimonio, sexualidad y divorcio desde el punto de vista evolutivo, para lo que ha investigado a lo largo de su carrera a 58 sociedades de todo el planeta. También es de una de las antropólogas más prestigiosas de Estados Unidos y autora de los ensayos ‘Por qué amamos’, ‘Anatomía del amor’ y ‘La ciencia del amor’, entre otros.},
  editortype = {director}
}

